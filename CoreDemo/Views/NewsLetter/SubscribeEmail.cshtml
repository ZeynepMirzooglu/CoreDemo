



<div class="tech-btm">
   
    <p> Blogları email ile alın.</p>
    <form  method="post">
        <input type="email" placeholder="Email" id="txtmail"  required="" name="Email">
        <input type="submit" id="btn" value="Abone Ol">
    </form>

</div>
<script type="text/javascript">
    $("#btn").click(function(){
        let values={
            Email: $("#txtmail").val()
        };
        $.ajax({
            type:"post",
            url: "/NewsLetter/SubscribeEmail/",
            data:values,
            success: function(func){
                let result=jQuery.parseJSON(func);
            }
        });
    });
    </script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" type="text/javascript"></script>